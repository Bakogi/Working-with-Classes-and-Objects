#include <iostream>
#include <string>

class Person {
private:
    std::string name;
    int age;

public:
    // Constructor
    Person(std::string n, int a) {
        name = n;
        age = a;
    }

    // Member function to display information
    void displayInfo() {
        std::cout << "Name: " << name << ", Age: " << age << std::endl;
    }
};

int main() {
    Person person1("Alice", 25);
    Person person2("Bob", 30);

    person1.displayInfo();
    person2.displayInfo();

    return 0;
}
